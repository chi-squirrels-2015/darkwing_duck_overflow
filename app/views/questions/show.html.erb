<div class="q_and_answer">
<h1><%=@question.title%></h1>
  <%= @question.votes.count %>
  <%= @question.content %>
  <%= @question.answers.each do |answer| %>
    <%= answer.votes.count %>
    <%= answer.content %>
  <% end %>
<div/>
<% if current_user %>
<button id="answer_button">Add an Answer</button>
  <div class="answer_form">
   <%= render :partial => '/answers/answer_form' %>
  </div>
<% end %>
